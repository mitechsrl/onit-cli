{
    "name": "@mitech/onit-cli",
    "description": "Onit dev tool CLI",
    "version": "3.0.0",
    "main": "./dist/index.js",
    "engines": {
        "node": ">=14.0.0"
    },
    "bin": {
        "onit": "dist/index.js"
    },
    "scripts": {
        "lint": "eslint . --fix --ext .ts",
        "clean": "rimraf .tsbuildinfo  dist",
        "copy-files": "copyfiles -u 1 src/**/*.js src/**/*.html src/**/*.ejs src/**/*.conf src/**/*.sh src/**/*.md src/**/*.json dist/",
        "scan-commands": "node ./dist/lib/generateScanResultFile.js",
        "tsc": "tsc",
        "build": "run-s clean tsc scan-commands copy-files",
        "start": "node ./dist/index.js"
    },
